{
  "name": "nest-typed-config",
  "version": "2.1.1",
  "description": "Intuitive, type-safe configuration module for Nest framework",
  "author": "Nikaple Zhou",
  "license": "MIT",
  "url": "https://github.com/Nikaple/nest-typed-config",
  "homepage": "https://github.com/Nikaple/nest-typed-config",
  "files": [
    "index.js",
    "index.d.ts",
    "index.ts",
    "lib",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Nikaple/nest-typed-config"
  },
  "bugs": {
    "url": "https://github.com/Nikaple/nest-typed-config/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf -rf dist && tsc -p tsconfig.build.json",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage",
    "doc": "typedoc lib/index.ts --tsconfig tsconfig.build.json",
    "lint": "eslint {lib/**/*.ts,tests/**/*.ts,examples/**/*.ts} --fix",
    "lint:dontfix": "eslint {lib/**/*.ts,test/**/*.ts,examples/**/*.ts}",
    "format": "prettier --write .",
    "format:dontfix": "prettier --check .",
    "prepublish:npm": "npm run build",
    "prerelease": "npm run build",
    "release": "semantic-release"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "debug": "4.3.1",
    "lodash.frompairs": "4.0.1",
    "lodash.merge": "4.6.2",
    "lodash.set": "4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@iarna/toml": "2.2.5",
    "@latipun7/commitlintrc": "1.1.0",
    "@latipun7/releaserc": "2.1.1",
    "@nestjs/axios": "0.0.3",
    "@nestjs/cli": "7.6.0",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/testing": "7.6.15",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.23",
    "@types/lodash.frompairs": "4.0.6",
    "@types/lodash.merge": "4.6.6",
    "@types/lodash.set": "4.3.6",
    "@types/node": "7.10.14",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "cosmiconfig": "7.0.0",
    "dotenv": "9.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "27.0.0-next.9",
    "lint-staged": "10.5.4",
    "parse-json": "5.2.0",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.0-next.12",
    "typedoc": "0.20.36",
    "typescript": "4.2.4",
    "yaml": "1.10.2"
  },
  "optionalDependencies": {
    "@iarna/toml": "^2.2.5",
    "@nestjs/axios": "^0.0.3",
    "cosmiconfig": "^7.0.0",
    "dotenv": "^9.0.0",
    "dotenv-expand": "^5.1.0",
    "parse-json": "^5.2.0",
    "yaml": "^1.10.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 6.10.0 < 9",
    "reflect-metadata": "^0.1.12",
    "rxjs": ">= 6.0.0 < 8"
  },
  "commitlint": {
    "extends": [
      "@latipun7/commitlintrc"
    ]
  },
  "release": {
    "extends": [
      "@latipun7/releaserc"
    ]
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*": "prettier --write --ignore-unknown"
  }
}
