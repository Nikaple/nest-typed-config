{
  "name": "nest-typed-config",
  "version": "1.2.0",
  "description": "Intuitive, type-safe configuration module for Nest framework",
  "author": "Nikaple Zhou",
  "license": "MIT",
  "url": "https://github.com/Nikaple/nest-typed-config",
  "homepage": "https://github.com/Nikaple/nest-typed-config",
