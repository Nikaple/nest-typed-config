    "class-validator": "^0.13.1",
    "cosmiconfig": "^7.0.0",
    "debug": "^4.3.1",
    "dotenv": "^9.0.0",
    "dotenv-expand": "^5.1.0",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "parse-json": "^5.2.0",
    "yaml": "^1.10.2"
